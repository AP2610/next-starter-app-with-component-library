{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "coverage/**"
      ]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "transit": {
      "dependsOn": [
        "^transit"
      ]
    },
    "check-types": {
      "dependsOn": [
        "transit"
      ]
    }
  }
}